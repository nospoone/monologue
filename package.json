{
	"name": "monologue",
	"productName": "Monologue",
	"version": "0.1.0",
	"main": "main.js",
	"license": "MIT",
	"repository": "nospoone/monologue",
	"electronVersion": "0.36.0",
	"scripts": {
		"prestart": "gulp less && gulp jade",
		"start": "electron main.js",
		"test": "xo",
		"build": "electron-packager . monologue --out=dist --ignore='^/dist$' --prune --asar --all --version=0.36.0"
	},
	"dependencies": {
		"electron-prebuilt": "^0.36.0",
		"tsv": "^0.2.0"
	},
	"devDependencies": {
		"electron-packager": "^5.0.0",
		"gulp": "^3.9.1",
		"gulp-jade": "^1.1.0",
		"gulp-less": "^3.0.5",
		"xo": "^0.13.0"
	},
	"xo": {
		"esnext": true,
		"envs": [
			"node",
			"browser"
		],
		"ignores": [
			"**/lib/**"
		],
		"rules": {
			"linebreak-style": 0
		}
	}
}
